	import uniLoadMore from '@/components/uni-load-more/uni-load-more.vue';
	import uniPopup from '@/components/uni-popup/uni-popup.vue';
	import Footer from '@/components/footer.vue'
	export default {
		components: {
			uniLoadMore,
			uniPopup,
			Footer
		},
		data() {
			return {
				num: 1,
				tabCurrentIndex: 0,
				// 全部状态的默认选中项
				stateCruuent: 1,
				navList: [{
						id: 0,
						text: '买入',
						loadingType: 'more',
						orderList: [],
						start: 0,
						total: null,
					},
					{
						id: 1,
						text: '卖出',
						loadingType: 'more',
						orderList: [],
						start: 0,
						total: null,
					},
					{
						id: 2,
						text: '订单',
						loadingType: 'more',
						orderList: [],
						start: 0,
						total: null,
					}
				],
				// 订单的全部状态的数据
				orderList: [{
						oId: 1,
						text: '全部',
					},
					{
						oId: 2,
						text: '待接单',
					},
					{
						oId: 3,
						text: '未支付',
					},
					{
						oId: 4,
						text: '已支付',
					},
					{
						oId: 5,
						text: '已完成',
					},
					{
						oId: 6,
						text: '已取消',
					},
					{
						oId: 7,
						text: '申诉中',
					},
					{
						oId: 8,
						text: '拒绝接单',
					},
					{
						oId: 9,
						text: '接单超时',
					},
				],
				// 付款方式
				paymentName: [{
						type: 1,
						text: '支付宝',
					},
					{
						type: 2,
						text: '微信',
					},
				],
				// 数量
				numberList: [{
						num: '全部',
						numId: 1,
					},
					{
						num: '>1000',
						numId: 2,
					},
					{
						num: '>10000',
						numId: 3,
					},
					{
						num: '>100000',
						numId: 4,
					},
				],
				muqian: {
					name: '支付宝',
					img: '../../../static/vip/pay3.png'
				},
				payWay: [{
						name: '支付宝',
						img: '../../../static/vip/pay3.png'
					},
					{
						name: '微信',
						img: '../../../static/vip/pay2.png'
					}
				],
				specClass: 'none',
				isflag: false,
				payFlag: true,
				// 支付方式默认选中项
				payment: 1,
				// 数量的默认选中项
				paymentNum: 1,
			};
		},

		onLoad(options) {
			this.getData()
		},
		onShow() {
			
		},
		onReachBottom(){
			
		},
		methods: {
			jumpDetail(id, status) {
				let pid = id.OrderID;
				uni.navigateTo({
					url: `/pages/order/orderDetail?orderId=${pid}&status=${status}`,
				});
			},
			jumppj(itemGood) {
				console.log(itemGood);
				var itemGood = JSON.stringify(itemGood);
				uni.navigateTo({
					url: '/pages/evaluate/releaseComments?itemGood=' + itemGood,
				});
			},
			async getData() {
				let start = this.navList[this.tabCurrentIndex].start
				let index = this.tabCurrentIndex
				let type = index
				if(type == 2){
					type = ""
				}
				let data = {
					Offset: Math.ceil(start / 10), //页面
					RowNumber: 10 ,//条数
					TransactionType:type
				}
				let stringData = JSON.stringify(data)
				let param = {
					action: 'ReadFishOrder',
					data: stringData
				}
				let res = await this.$api.api.getFish(param)
				if (res.data.status == true) {
					res.data.data.forEach((item, index) => {
						item.FishPrice = Number(item.FishPrice).toFixed(2)
						item.FishNumber = Number(item.FishNumber).toFixed(2)
						item.total = Number(item.FishPrice * item.FishNumber).toFixed(2)
					})
					this.navList[index].total = res.data.sum
					this.navList[index].start += res.data.data.length
					this.navList[index].orderList = this.navList[index].orderList.concat(res.data.data)
					this.navList[index].loadingType = this.navList[index].total > this.navList[index].start ? 'more' : 'nomore';
				}
			},
			//swiper 切换
			changeTab(e) {
				this.tabCurrentIndex = e.target.current;
				this.getData();
			},
			//顶部tab点击
			tabClick(index) {
				this.tabCurrentIndex = index;
			},
			// 全部状态的选中项的的改变
			changeState(id) {
				this.stateCruuent = id;
			},
			// 点击打开全部状态的弹出框
			stateOpen() {
				this.$refs.statePopup.open();
			},
			// 点击打开筛选的弹出框
			openScreen() {
				this.$refs.popup.open();
			},
			// 点击更改支付方式
			changePayment(type) {
				this.payment = type;
			},
			// 点击更换数量
			changeNum(id) {
				this.paymentNum = id;
			},
			// 筛选的重置按钮
			screenReset() {
				this.payment = 1;
				this.paymentNum = 1;
			},
			// 根据条件 打开或关闭 买卖的弹框
			toggleSpec() {
				if (this.specClass === 'show') {
					this.specClass = 'hide';
					setTimeout(() => {
						this.specClass = 'none';
					}, 250);
				} else if (this.specClass === 'none') {
					this.specClass = 'show';
				}
			},

			// 跳转退款页面
			submit() {
				this.specClass = 'none';
			},
			choose(index) {
				this.muqian = this.payWay[index]
				this.isflag = false
			},
			scroll() {
				this.isflag = !this.isflag
			},
			stopPrevent() {}
		},
	};
</script>

