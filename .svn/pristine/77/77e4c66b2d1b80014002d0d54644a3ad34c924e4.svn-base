						text: '订单',
						list: [],
						loadingType: 'more',
						start: 0,
						total: null
					},
				],
				// 付款方式
				paymentName: [{
						type: 1,
						text: '支付宝',
					},
					{
						type: 2,
						text: '微信',
					},
				],
				// 数量
				numberList: [{
						num: '全部',
						numId: 1,
					},
					{
						num: '>1000',
						numId: 2,
					},
					{
						num: '>10000',
						numId: 3,
					},
					{
						num: '>100000',
						numId: 4,
					},
				],
				// 订单的全部状态的数据
				orderList: [{
						oId: 1,
						text: '全部',
					},
					{
						oId: 2,
						text: '待接单',
					},
					{
						oId: 3,
						text: '未支付',
					},
					{
						oId: 4,
						text: '已支付',
					},
					{
						oId: 5,
						text: '已完成',
					},
					{
						oId: 6,
						text: '已取消',
					},
					{
						oId: 7,
						text: '申诉中',
					},
					{
						oId: 8,
						text: '拒绝接单',
					},
					{
						oId: 9,
						text: '接单超时',
					},
				],
				// tab 栏默认选中项
				current: 0,
				// 手机的屏幕高度
				phoneHeight: 0,
				// 支付方式默认选中项
				payment: 1,
				// 数量的默认选中项
				paymentNum: 1,
				// 全部状态的默认选中项
				stateCruuent: 1,
				specClass: 'none',
				isflag: false,
				payFlag: true,
				muqian: {
					name: '支付宝',
					img: '../../../static/vip/pay3.png'
				},
				payWay: [{
						name: '支付宝',
						img: '../../../static/vip/pay3.png'
					},
					{
						name: '微信',
						img: '../../../static/vip/pay2.png'
					}
				]
			};
		},
		onLoad() {
			uni.getSystemInfo({
				success: (res) => {
					this.phoneHeight = res.screenHeight * 2 - 220;
					console.log(res.screenHeight);
				},
			});
		},
		onReachBottom() {
			console.log(11)
			this.getList()
		},
		methods: {
			jump() {
				uni.navigateTo({
					url: '/pages/integralPages/OTCBusiness/otcOrderDetail'
				})
			},
			// 点击切换 tab 栏的时候
			tabChange(id) {
				this.current = id;
			},
			// 滑动 tab 栏下面的数据
			changeSwiper(e) {
				this.current = e.target.current;
				if (this.current == 2) {
					this.getList()
				}
			},
			async getList() {
				let start = this.tabText[this.current].start
				let index = this.current
				let data = {
					Offset: Math.ceil(start / 10), //页面
					RowNumber: 10 //条数
				}
				let stringData = JSON.stringify(data)
				let param = {
					action: 'ReadFishOrder',
					data: stringData
				}
				let res = await this.$api.api.getFish(param)
				if (res.data.status == true) {
					res.data.data.forEach((item, index) => {
						item.total = Number(item.FishPrice * item.FishNumber).toFixed(2)
					})
					this.tabText[index].total = res.data.sum
					this.tabText[index].start += res.data.data.length
					this.tabText[index].list = this.tabText[index].list.concat(res.data.data)
					this.tabText[index].loadingType = this.tabText[index].total > this.tabText[index].start ? 'more' : 'nomore';
				}
			},
			// 点击打开筛选的弹出框
			openScreen() {
				this.$refs.popup.open();
			},
			// 点击更改支付方式
			changePayment(type) {
				this.payment = type;
			},
			// 点击更换数量
			changeNum(id) {
				this.paymentNum = id;
			},
			// 筛选的重置按钮
			screenReset() {
				this.payment = 1;
				this.paymentNum = 1;
			},
			// 确定筛选
			screenDetermine() {
				alert('确定筛选');
			},
			// 点击打开全部状态的弹出框
			stateOpen() {
				this.$refs.statePopup.open();
			},
			// 全部状态的选中项的的改变
			changeState(id) {
				this.stateCruuent = id;
			},
			// 根据条件 打开或关闭 买卖的弹框
			toggleSpec() {
				if (this.specClass === 'show') {
					this.specClass = 'hide';
					setTimeout(() => {
						this.specClass = 'none';
					}, 250);
				} else if (this.specClass === 'none') {
					this.specClass = 'show';
				}
			},
			scroll() {
				this.isflag = !this.isflag
			},
			submit() {
				this.specClass = 'none';
			},
			choose(index) {
				this.muqian = this.payWay[index]
				this.isflag = false
			},
			stopPrevent() {},
		},
	};
</script>

